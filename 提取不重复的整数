#include<stdio.h>

#include<string.h>


using namespace std;

int main(){
    char a[100] = {0};
    scanf("%s",&a);
    char b[100] = {0};
    int len = strlen(a)-1;
    for(int i=len; i>=0; i--){
        if(b[a[i]] == 0){
            printf("%c",a[i]);
            b[a[i]]++;
        }
    }
    
    return 0;
}
